{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "no-console": 0,
    "no-loops/no-loops": 0,
    "no-unused-vars": 0,
    "@typescript-eslint/no-unused-vars": 0,
    "prefer-const": 0,
    "no-redeclare": "error"
  }
}
