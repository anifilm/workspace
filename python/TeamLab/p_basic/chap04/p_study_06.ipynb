    "decimal = 10\n",
    "result = ''\n",
    "while decimal > 0:\n",
    "    remainder = decimal % 2\n",
    "    decimal = decimal // 2  # 나눗셈 정수 연산 (소수점 이하를 버림)\n",
    "    result = str(remainder) + result\n",
    "print(result)"
